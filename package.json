{
  "name": "flow-ide",
  "private": true,
  "version": "1.9.0",
  "description": "Flowtype support in Atom without any bloatware",
  "main": "lib/index.js",
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "package-deps": [
    "linter",
    "hyperclick"
  ],
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "consumedServices": {
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steelbrain/flow-ide.git"
  },
  "keywords": [
    "flow",
    "flowtype",
    "ide",
    "javascript"
  ],
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steelbrain/flow-ide/issues"
  },
  "homepage": "https://github.com/steelbrain/flow-ide#readme",
  "dependencies": {
    "atom-autocomplete": "^1.0.0",
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.4.1",
    "flow-language-server": "^0.2.3",
    "sb-string_score": "^0.1.20"
  },
  "devDependencies": {
    "eslint-config-steelbrain": "^3.0.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "flow-bin": "^0.57.3"
  },
  "configSchema": {
    "onlyIfAppropriate": {
      "title": "Only activate when .flowconfig exists",
      "type": "boolean",
      "default": true
    },
    "showUncovered": {
      "title": "Show uncovered code in the editor",
      "type": "boolean",
      "default": false
    },
    "executablePath": {
      "description": "Path to `flow` executable",
      "type": "string",
      "default": ""
    },
    "hyperclickPriority": {
      "description": "Priority to use for hyperclick provider (requires restart)",
      "type": "integer",
      "default": "0"
    }
  }
}
